{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*\\.(?:html|css|js|png|jpg|jpeg|gif|ico|svg|json))",
      "dest": "frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}